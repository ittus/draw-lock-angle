{
  "name": "draw-lock-angle",
  "author": "Vu Minh Thang",
  "version": "0.0.3",
  "description": "Lock 45, 90, 135, 180... degree when drawing line on Javascript canvas",
  "private": false,
  "license": "MIT",
  "main": "dist/src/libs/helper.js",
  "files": [
    "dist/libs/*"
  ],
  "homepage": "https://github.com/ittus/draw-lock-angle",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ittus/draw-lock-angle"
  },
  "keywords": [
    "canvas",
    "draw",
    "angle",
    "lock"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn build:docs && yarn build:lib",
    "lint": "vue-cli-service lint",
    "build:docs": "vue-cli-service build --dest dist/docs",
    "test": "vue-cli-service test:unit tests/**/*.spec.js",
    "build:lib": "babel ./src/libs/helper.js --presets babel-preset-es2015 --out-dir dist/"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "konva": "^2.6.0",
    "vue": "^2.6.6",
    "vue-konva": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-unit-jest": "^3.4.1",
    "@vue/cli-service": "^3.4.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.1.0",
    "jest-runtime": "^24.1.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
